  "$defs": {
    "BoilerModel": {
      "type": "object",
      "properties": {
        "powerBurner": {
          "type": "number",
          "unit": "%",
          "ui": {
            "group": 2,
            "editor": {
              "editorType": "dxNumberBox",
              "label": {
                "location": "top",
                "showColon": true,
                "text": "Мощность горелки котла"
              },
              "editorOptions": {
                "readOnly": true
              }
            }
          }
        },
        "mode": {
          "$ref": "#/$defs/WorkMode",
          "ui": {
            "group": 2,
            "editor": {
              "editorType": "dxSelectBox",
              "label": {
                "location": "top",
                "showColon": true,
                "text": "Режим работы котла"
              },
              "editorOptions": {
                "readOnly": true,
                "valueExpr": "id",
                "displayExpr": "description"
              }
            }
          }
        },
        "status": {
          "$ref": "#/$defs/BoilerStatus",
          "ui": {
            "group": 2,
            "editor": {
              "editorType": "dxSelectBox",
              "label": {
                "location": "top",
                "showColon": true,
                "text": "Состояние котла"
              },
              "editorOptions": {
                "readOnly": true,
                "valueExpr": "id",
                "displayExpr": "description"
              }
            },
            "colorizer": {
              "styleProps": [
                {
                  "fill": {
                    "#00FF00": [
                      3,
                      4,
                      2
                    ],
                    "red": [
                      6
                    ],
                    "white": [
                      0,
                      1,
                      5,
                      8,
                      9,
                      10
                    ]
                  }
                }
              ]
            }
          }
        },
        "alarm": {
          "$ref": "#/$defs/BoilerAlarm",
          "ui": {
            "group": 2,
            "editor": {
              "editorType": "dxSelectBox",
              "label": {
                "location": "top",
                "showColon": true,
                "text": "Авария котла"
              },
              "editorOptions": {
                "readOnly": true,
                "valueExpr": "id",
                "displayExpr": "description"
              }
            }
          }
        }
      },
      "required": [
        "powerBurner",
        "mode",
        "status",
        "alarm"
      ]
    },
    "BoilerStatus": {
      "type": "integer",
      "enum": [
        0,
        1,
        2,
        3,
        4,
        5,
        6,
        8,
        9,
        10
      ],
      "enumDescriptions": {
        "0": "STOPPED - Остановлен",
        "1": "OPENING_OUTLET_VALVE - Открытие задвижки",
        "2": "BURNER_STARTUP - Запуск горелки",
        "3": "OPERATION_LOW_BURN - Работа (малое горение)",
        "4": "OPERATION_REGULATION - Работа (регулирование)",
        "5": "BURNER_STOP - Останов горелки",
        "6": "ALARM - Авария",
        "8": "HOT_RESERVE - Горячий резерв",
        "9": "CLOSING_OUTLET_VALVE - Закрытие задвижки",
        "10": "NOT_READY - Нет готовности"
      }
    },
    "BoilerAlarm": {
      "type": "integer",
      "enum": [
        0,
        1,
        2,
        4,
        8,
        16,
        32,
        64,
        128,
        256,
        512,
        1024,
        2048,
        4096
      ],
      "enumDescriptions": {
        "0": "NORM - Норма",
        "1": "TEMP_SENSOR_FAULT - Неисправность (датчик температуры на выходе)",
        "2": "BURNER_ALARM - Авария (горелка)",
        "4": "PRESSURE_SENSOR_FAULT - Неисправность (датчик давления)",
        "8": "SAFETY_CIRCUIT - Цепь безопасности",
        "16": "EMERGENCY_STOP - Аварийный останов",
        "32": "HIGH_TEMP_THRESHOLD - Аварийный порог (высокая температура)",
        "64": "HIGH_PRESSURE_THRESHOLD - Аварийный порог (высокое давление)",
        "128": "LOW_PRESSURE_THRESHOLD - Аварийный порог (низкое давления)",
        "256": "NO_FLOW - Нет протока",
        "512": "PUMPS_DISCONNECTED - Отключение насосов",
        "1024": "BURNER_IGNITION_TIMEOUT - Таймаут (розжиг горелки)",
        "2048": "SPONTANEOUS_BURNER_SHUTDOWN - Самопроизв. отключение (горелка)",
        "4096": "VALVE_ALARM - Авария (задвижка)"
      }
    },
    "FlapModel": {
      "type": "object",
      "properties": {
        "mode": {
          "$ref": "#/$defs/FlapMode",
          "ui": {
            "group": 5,
            "editor": {
              "editorType": "dxSelectBox",
              "label": {
