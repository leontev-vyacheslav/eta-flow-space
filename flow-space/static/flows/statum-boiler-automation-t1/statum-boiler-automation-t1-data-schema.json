              "editorOptions": {
                "readOnly": true,
                "valueExpr": "id",
                "displayExpr": "description"
              }
            },
            "colorizer": {
              "styleProps": [
                {
                  "fill": {
                    "#00FF00": [
                      0
                    ],
                    "yellow": [
                      1
                    ]
                  }
                }
              ]
            }
          }
        }
      },
      "required": [
        "mode"
      ]
    },
    "FlapMode": {
      "type": "integer",
      "enum": [
        0,
        1
      ],
      "enumDescriptions": {
        "0": "AUTO - Авто",
        "1": "MANUAL - Ручной"
      }
    },
