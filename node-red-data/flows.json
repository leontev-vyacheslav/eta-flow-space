        "z": "a7c135e772de0725",
        "name": "initDatabaseContext",
        "func": "const { Sequelize, DataTypes } = global.get('sequelize');\n\nlet sequelize = flow.get('sequelize');\nif (!sequelize) {\n    sequelize = new Sequelize('postgres://postgres:0987654321@node-red-database:5432/eta_nodered_db');\n    flow.set('sequelize', sequelize);\n}\n\nconst SewageStateDataModel = sequelize.define('SewageState', {\n    state: DataTypes.STRING,\n    createdAt: DataTypes.DATE,\n    updatedAt: DataTypes.DATE,\n}, {\n    tableName: 'sewage_state',\n},);\n\nflow.set(\"SewageStateDataModel\", SewageStateDataModel)\n\n\nreturn msg;",
